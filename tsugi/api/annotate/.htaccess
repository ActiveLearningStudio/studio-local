<IfModule mod_rewrite.c>
    RewriteEngine on
    RewriteRule "(^|/)\." - [F]
    RewriteCond %{REQUEST_URI} !=/favicon.ico
    RewriteCond %{REQUEST_URI} !\.(htm|jpg|csv|xls|png|css|js|php)$
    RewriteRule ^ index.php [L]
</IfModule>

<IfModule !mod_rewrite.c>
    FallbackResource index.php
</IfModule>
